{
  "manifest_version": 3,
  "name": "Hide Google Meet Addons",
  "version": "0.1.0",
  "description": "Hides addon installation suggestions in Google Meet chat.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["script/content.js"],
      "run_at": "document_idle"
    }
  ]
}